{
    "name": "marlemiesz/google-search-results",
    "description": "Scrape Google search result",
    "keywords": [
        "marlemiesz",
        "google-search-results"
    ],
    "homepage": "https://github.com/marlemiesz/google-search-results",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcin Lemiesz",
            "email": "mar.lemiesz@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpunit/phpunit": "^9.0",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Marlemiesz\\GoogleSearchResult\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Marlemiesz\\GoogleSearchResult\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
